{
  "name": "device-management-api",
  "version": "0.0.1",
  "description": "[TPI documentation](../README.md)",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "lint": "eslint src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "doc:code": "jsdoc -d ./docs/html --debug -r \"./src/\" -t node_modules/minami -R ./README.md",
    "doc:api:folder": "mkdirp docs/api",
    "doc:api:create": "swagger-jsdoc -d swaggerDef.js -o docs/api/eagle-api.yml src/**/*.js",
    "doc:api:validate": "lint-openapi -d docs/api/eagle-api.yml",
    "doc:api": "npm run doc:api:folder && npm run doc:api:create && npm run doc:api:validate"
  },
  "keywords": [
    "TPI",
    "API"
  ],
  "author": "Luca Saccone <luca.saccone@hach.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "ibm-openapi-validator": "^0.2.4",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "nodemon": "^1.18.11",
    "swagger-jsdoc": "^3.2.8"
  }
}